    "jest-canvas-mock": "^2.2.0",
    "raw-loader": "^3.1.0",
    "stylelint": "^15.10.1",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-csstree-validator": "^1.8.0",
    "stylelint-scss": "^3.17.2",
    "terser-webpack-plugin": "^2.2.1",
